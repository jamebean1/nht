event clicked;//***********************************************************************************************
//
//  Event:   clicked 
//  Purpose: To allow the user to edit an existing document
//  
//  Date     Developer   Description
//  -------- ----------- -----------------------------------------------------------------------
//  03/30/01 C. Jackson  Original Version
//  12/03/01 C. Jackson  Set Boolean to determine whether or not the edit window can be opened.
//  12/10/01 C. Jackson  Make sure there is a document before trying to open the edit window.
//				
//***********************************************************************************************

STRING l_cDocName, l_cDocExt, l_cFullName, l_cMsg
LONG l_nIndex, l_nCounter, l_nDocNameLength, l_nDWRow
BOOLEAN l_bExists, l_bDeleted, l_bAppOpen, l_bOKToOpen = TRUE
S_DOCFIELDPROPERTIES l_sparms
U_DW_STD l_dwMatchedRecords

l_dwMatchedRecords = w_table_maintenance.uo_document_registration.dw_matched_records

l_nDWRow = l_dwMatchedRecords.GetRow()

IF l_nDWRow > 0 THEN
	l_cDocName = l_dwMatchedRecords.GetItemString(l_nDWRow,'documents_doc_filename')
	
	IF NOT ISNULL(l_cDocName) THEN
		
		// Check to see if this doc is already open
		l_cFullName = 'C:'+l_cDocName
		IF FileExists(l_cFullName) THEN
			
			// Attempt to delete the file
			l_bDeleted = FileDelete(l_cFullName)
			
			// If unsuccessful, since the file does exist, this means the file is open and cannot be deleted
			IF NOT l_bDeleted THEN
	
				l_nDocNameLength = LEN(l_cDocName)
				l_cDocExt = UPPER(MID(l_cDocName, (l_nDocNameLength - 3)))
				
				CHOOSE CASE l_cDocExt
					CASE '.PDF'
				
						l_cMsg = 'The Adobe Acrobat file "' + l_cDocName + '" associated with this document, is currently open.  ' + &
									 'Please close before editing this document. '
						l_bOKToOpen = FALSE
						l_bAppOpen = TRUE
								 
					CASE '.DOC'
						
						l_cMsg = 'The Word file "' + l_cDocName + '" associated with this document, is currently open.  ' + &
									 'Please close before editing this document. '
						l_bOKToOpen = FALSE
						l_bAppOpen = TRUE
									 
					CASE '.XLS'
						
						l_cMsg = 'The Excel file "' + l_cDocName + '" associated with this document, is currently open.  ' + &
									 'Please close before editing this document. '
						l_bOKToOpen = FALSE
						l_bAppOpen = TRUE
						
				END CHOOSE
				
				
				IF l_bAppOpen THEN
					messagebox(gs_AppName,l_cMsg)
					RETURN
				END IF
				
			ELSE
				
				l_bOKToOpen = TRUE
			
			END IF
			
		END IF
		
	END IF
			
	
	IF l_bOKToOpen THEN
		FWCA.MGR.fu_OpenWindow(w_doc_reg_detail_edit,l_sParms)
	END IF
			
END IF
end event

