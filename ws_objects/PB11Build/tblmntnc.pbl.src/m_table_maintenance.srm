event clicked;/****************************************************************************************

			Event:	clicked
			Purpose:	To clear the Search Criteria DataWindow

****************************************************************************************/

STRING l_cDocTypeID, l_cNewFieldLabel, l_cColName, l_cColFormatID, l_cColFormat, l_cModLabelString1
STRING l_cModLabelString2, l_cModLabelVisible, l_cModDataVisible, l_cModEditMask
LONG l_nRecordCount, l_nCounter, l_nRow

W_TABLE_MAINTENANCE l_wParentWindow

//-------------------------------------------------------------------------------------
//
//		Get the Active Sheet and place it in the instance varaible.  Call the newsearch
//		function on the parentwindow.
//
//-------------------------------------------------------------------------------------

l_wParentWindow = FWCA.MGR.i_MDIFrame.GetActiveSheet()

IF IsValid(l_wParentWindow) THEN
	l_wParentWindow.fw_clearsearch()
	IF NOT w_table_maintenance.i_bAddDocCancel THEN
		// If Document Registration, then set up for initial search
		IF w_table_maintenance.i_bDocReg THEN
			// Grab the Document Type 
			l_cDocTypeID = w_table_maintenance.uo_document_registration.dw_doc_reg_type.GetText()
			
			// Count the number of records for this DocTypeID
				SELECT max(doc_field_order) INTO :l_nRecordCount
				  FROM cusfocus.document_fields
				 WHERE doc_type_id = :l_cDocTypeID
				 USING SQLCA;
			
			l_nCounter = 1
			
			DO WHILE l_nCounter <= l_nRecordCount
				
				// Initialize field label
				SETNULL(l_cNewFieldLabel)
				
				// Get the field label from document_fields table
				SELECT  doc_field_label, doc_column_name, doc_format_id INTO :l_cNewFieldLabel, :l_cColName, :l_cColFormatID
				  FROM cusfocus.document_fields
				 WHERE doc_type_id = :l_cDocTypeID
					AND doc_field_order = :l_nCounter
				 USING SQLCA;
			
				// Get the Format Type
				SELECT edit_mask INTO :l_cColFormat
				  FROM cusfocus.display_formats
				 WHERE format_id = :l_cColFormatID
				 USING SQLCA;
				 
				IF NOT ISNULL(l_cNewFieldLabel) THEN
					l_cModLabelString1 = "doc_config_"+string(l_nCounter)+"_t.text = '"+l_cNewFieldLabel+":'"
					l_cModLabelString2 = "doc_config_"+string(l_nCounter)+"_t.text = '"+l_cNewFieldLabel+"'"
					l_cModLabelVisible = "doc_config_"+string(l_nCounter)+"_t.Visible=1"
					l_cModDataVisible = "doc_config_"+string(l_nCounter)+".Visible=1"
					w_table_maintenance.uo_document_registration.dw_search_criteria.Modify(l_cModLabelString1)
					w_table_maintenance.uo_document_registration.dw_search_criteria.Modify(l_cModLabelVisible)
					w_table_maintenance.uo_document_registration.dw_search_criteria.Modify(l_cModDataVisible)
					w_table_maintenance.uo_document_registration.dw_matched_records.Modify(l_cModLabelString2)
					w_table_maintenance.uo_document_registration.dw_matched_records.Modify(l_cModLabelVisible)
					w_table_maintenance.uo_document_registration.dw_matched_records.Modify(l_cModDataVisible)
					IF NOT ISNULL(l_cColFormat) and l_cColFormat <> "" THEN
						l_cModEditMask = "doc_config_"+string(l_nCounter)+".EditMask.Mask='"+l_cColFormat+"'"
						w_table_maintenance.uo_document_registration.dw_search_criteria.Modify(l_cModEditMask)
					END IF
				END IF
				
				l_nCounter++
				
			LOOP
		
			// Set the hidden doc_type_id field on l_wParentWindow.dw_search_criteria to that selected in the drop-down above
			l_nRow = w_table_maintenance.uo_document_registration.dw_search_criteria.GetRow()
			IF l_nRow > 0 THEN
				w_table_maintenance.uo_document_registration.dw_search_criteria.SetItem(l_nRow,"doc_type_id",l_cDocTypeID)
				w_table_maintenance.uo_document_registration.dw_search_criteria.SetFocus()
			END IF
			
		END IF
	end if
	
END IF


end event

