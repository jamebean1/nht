		Purpose:	To print either the Case Correspondence or Case Reminder.

***************************************************************************************/

LONG							l_nRows[]
S_DOC_INFO					l_sDocInfo[]
U_DW_STD						l_dwDocInfo
W_CREATE_MAINTAIN_CASE	l_wParentWindow

l_wParentWindow = FWCA.MGR.i_MDIFrame.GetActiveSheet()

IF IsValid(l_wParentWindow) THEN
	IF l_wParentWindow.dw_folder.i_SelectedTab = 7 THEN
		IF IsValid(l_wParentWindow.i_uoCaseCorrespondence) THEN
			l_dwDocInfo = l_wParentWindow.i_uoCaseCorrespondence.dw_correspondence_list
			l_dwDocInfo.fu_GetSelectedRows (l_nRows[])
			IF Upperbound(l_nRows) > 0 THEN
				// gather correspondence details
				l_sDocInfo[1].a_cDocID = l_dwDocInfo.GetItemString (l_nRows[1], 'correspondence_id')
				l_sDocInfo[1].a_cDocName = l_dwDocInfo.GetItemString (l_nRows[1], 'letter_types_letter_name')
				l_sDocInfo[1].a_cCaseNumber = l_wParentWindow.i_cCurrentCase
				l_sDocInfo[1].a_cCaseType = l_wParentWindow.i_cCaseType
				l_sDocInfo[1].a_dtSent = l_dwDocInfo.GetItemDateTime (l_nRows[1], 'correspondence_corspnd_sent')
				IF l_dwDocInfo.GetItemString (l_nRows[1], 'correspondence_corspnd_doc_filled') = 'Y' THEN
					l_sDocInfo[1].a_bFilled = TRUE
				ELSE
					l_sDocInfo[1].a_bFilled = FALSE
				END IF
				
				l_wParentWindow.i_uoCaseCorrespondence.i_uoDocMgr.i_dwRefreshDW = l_wParentWindow.i_uoCaseCorrespondence.dw_correspondence_detail
				l_wParentWindow.i_uoCaseCorrespondence.i_uoDocMgr.uf_processdocs (l_sDocInfo[], TRUE, FALSE)
			END IF
		END IF
	ELSEIF l_wParentWindow.dw_folder.i_SelectedTab = 8 THEN
		IF IsValid(l_wParentWindow) THEN
			IF IsValid(l_wParentWindow.i_uoCaseReminders) THEN
				l_wParentWindow.i_uoCaseReminders.fu_printcasereminder()
			END IF
		END IF
	END IF
END IF

end event

