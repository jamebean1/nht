event clicked;/***************************************************************************************


		Event:	clicked
		Purpose:	To open the Batch Correspondence window

***************************************************************************************/
W_CREATE_MAINTAIN_CASE	l_CMCase
WINDOW	l_wParentWindow
STRING ls_class

l_wParentWindow = FWCA.MGR.i_MDIFrame.GetActiveSheet()

IF IsValid(l_wParentWindow) THEN
	ls_class = ClassName(l_wParentWindow)
	
	IF ls_class <> "w_create_maintain_case" THEN
		l_wParentWindow = FWCA.MGR.i_MDIFrame.GetNextSheet(l_wParentWindow)

		IF IsValid(l_wParentWindow) THEN
			ls_class = ClassName(l_wParentWindow)
		END IF
	END IF
END IF	

IF ls_class = "w_create_maintain_case" THEN
	l_CMCase = l_wParentWindow
	IF IsValid(l_CMCase) THEN
		IF IsValid(l_CMCase.i_uoCaseCorrespondence) THEN
			IF IsValid(l_CMCase.i_uoCaseCorrespondence.i_uoDocMgr) THEN
				IF IsValid(l_CMCase.i_uoCaseCorrespondence.i_uoDocMgr.i_wDocWindow) THEN
					l_CMCase.i_uoCaseCorrespondence.i_uoDocMgr.uf_closedocwindow()
				END IF
			END IF
		END IF
	END IF
END IF
SetPointer(HOURGLASS!)
FWCA.MGR.fu_OpenWindow(w_batch_correspondence, -1)

end event

