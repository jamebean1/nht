//  -------- ----------- ---------------------------------------------------------------------
//  08/18/99 M. Caruso   If no rows remain, a new row is created in dw_case_reminder.
//  08/17/00 C. Jackson  Set the Delete flag to false after the delete is complete. (SCR 599)
//  12/4/2000 K. Claver  Added code to trigger the clicked event of the delete button on the 
//								 contact notes tab.
//  07/09/01 M. Caruso   Added code to determine which tab is active and delete the selected
//								 correspondence if that is the active tab.
//  02/05/02 M. Caruso   Updated code for deleting correspondece.
//  02/27/02 M. Caruso   Simplified code for deleting correspondece.
//********************************************************************************************
			
INT							l_nReturn, l_nCounter, l_nUpperBound, l_nResponse, l_nDebug
DATE							l_dBlankDate, l_dReminderDate
LONG							l_nRows[], l_nCurrentRow
STRING						l_cCorrespondenceID
U_DW_STD						l_dwDocList, l_dwDocDetails, l_dwReminderList, l_dwReminderDetails
W_CREATE_MAINTAIN_CASE	l_wParentWindow

i_bReminderDelete = TRUE

l_wParentWindow = FWCA.MGR.i_MDIFrame.GetActiveSheet()
IF IsValid(l_wParentWindow) THEN
	CHOOSE CASE l_wParentWindow.dw_folder.i_SelectedTab
		CASE 3		// contact notes tab
			IF IsValid( l_wParentWindow.i_uoContactNotes ) THEN
				l_wParentWindow.i_uoContactNotes.cb_delete.Event Trigger clicked( )
			END IF
			
		CASE 5		// case tab
			//only one thing to delete here.
			IF IsValid( w_create_maintain_case.i_uoCaseDetails.tab_folder.tabpage_carve_out ) THEN
				w_create_maintain_case.i_uoCaseDetails.tab_folder.tabpage_carve_out.fu_Delete( )
			END IF
			
		CASE 7		// correspondence tab
			IF ISValid(l_wParentWindow.i_uoCaseCorrespondence) THEN
				
				// initialize reference variables
				l_dwDocList = l_wParentWindow.i_uoCaseCorrespondence.dw_correspondence_list
				l_dwDocDetails = l_wParentWindow.i_uoCaseCorrespondence.dw_correspondence_detail
				
				// if no entries in list view, notify the user
				IF l_dwDocList.RowCount() = 0 THEN
					MessageBox(gs_AppName, "Sorry.  There are no rows to delete.")
					RETURN
				END IF
				
				// If the deleted row has a corresondence open for edit, do not allow deletion
				IF IsValid(l_wParentWindow.i_uoCaseCorrespondence) THEN
					IF IsValid(l_wParentWindow.i_uoCaseCorrespondence.i_uoDocMgr) THEN
						IF IsValid(l_wParentWindow.i_uoCaseCorrespondence.i_uoDocMgr.i_wDocWindow) THEN
							l_cCorrespondenceID = l_dwDocDetails.object.correspondence_id[l_dwDocDetails.i_CursorRow]
							IF l_wParentWindow.i_uoCaseCorrespondence.i_uoDocMgr.i_sCurrentDocInfo.a_cDocID = l_cCorrespondenceID THEN
								MessageBox(gs_AppName, "Cannot delete when the correspondence is open for edit.")
								RETURN
							END IF
						END IF
					END IF
				END IF

				// confirm the desire to delete the current record.
				l_nResponse = MessageBox (gs_appname, 'Are you sure you want to delete this correspondence entry?', &
													StopSign!, YesNo!, 2)
													
				IF l_nResponse = 1 THEN
					
					// process the deletion
					l_nRows[1] = l_dwDocDetails.i_CursorRow
					l_dwDocDetails.fu_Delete(1, l_nRows[], l_dwDocDetails.c_IgnoreChanges)
					l_dwDocDetails.fu_Save(l_dwDocDetails.c_SaveChanges)
			
					IF l_dwDocDetails.RowCount() > 0 THEN
			
						l_dwDocList.fu_Retrieve(l_dwDocList.c_IgnoreChanges, l_dwDocList.c_NoReselectRows)
						l_dwDocDetails.fu_View()
						
					ELSE
						
						l_dwDocDetails.fu_New(1)
						
					END IF
					
				END IF
				
			END IF
			
		CASE 8		// reminders tab
			l_dwReminderList = l_wParentWindow.i_uoCaseReminders.dw_case_reminder_history
			l_dwReminderDetails = l_wParentWindow.i_uoCaseReminders.dw_case_reminder
			
			IF ISValid(l_wParentWindow.i_uoCaseReminders) THEN
				IF l_dwReminderList.RowCount() = 0 THEN
					MessageBox(gs_AppName, "Sorry.  There are no rows to delete.")
					RETURN
				END IF
			
				l_nReturn = MessageBox(gs_AppName, "Are you really sure you want to delete this Reminder?", &
					QUESTION!, YESNO!)
		
				IF l_nReturn = 2 THEN
					RETURN
				END IF
		
				l_dReminderDate = Date(l_dwReminderList.GetItemDateTime(l_dwReminderList.i_CursorRow, 'reminder_set_date'))
		
				l_nUpperBound = UpperBound(l_wParentWindow.i_uoCaseReminders.i_dCaseReminderDates[])
		
				FOR l_nCounter = 1 to l_nUpperBound
					IF l_wParentWindow.i_uoCaseReminders.i_dCaseReminderDates[l_nCounter] = &
																					l_dReminderDate THEN
						l_wParentWindow.i_uoCaseReminders.i_dCaseReminderDates[l_nCounter] = &
						l_dBlankDate
						EXIT
					END IF
				NEXT
		
				l_wParentWindow.SetRedraw(FALSE)
					
				l_nRows[1] = l_dwReminderDetails.i_CursorRow
				l_dwReminderDetails.fu_Delete(1, l_nRows[], l_dwReminderDetails.c_IgnoreChanges)
				l_dwReminderDetails.fu_Save(l_dwReminderDetails.c_SaveChanges)
		
				IF l_dwReminderDetails.RowCount() > 0 THEN
		
					l_dwReminderList.fu_Retrieve(l_dwReminderList.c_IgnoreChanges, l_dwReminderList.c_NoReselectRows)
					l_dwReminderDetails.fu_View()
					
				ELSE
					
					l_dwReminderDetails.fu_New(1)
					
				END IF
				
				l_wParentWindow.i_uoCaseReminders.dw_calendar.fu_CalSetDates(&
					l_wParentWindow.i_uoCaseReminders.i_dCaseReminderDates[])
				l_wParentWindow.SetRedraw(TRUE)
				
				// Set the delete flag back to false
				m_create_maintain_case.i_bReminderDelete = FALSE	
			END IF
			
	END CHOOSE
	
END IF
end event

